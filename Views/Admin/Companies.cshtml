@model List<Company>

<div class="container">
    <div class="d-flex justify-content-start mb-4 mt-2">
        <a asp-controller="Company" asp-action="Upsert" class="btn btn-success">
            Add new
        </a>
    </div>
    <table id="tblData2" class="table table-striped w-100">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Street Address
                </th>
                <th>
                    City
                </th>
                <th>
                    State
                </th>
                <th>
                    Postal code
                </th>
                <th>
                    Phone number
                </th>
                <th>

                </th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/company.js"></script>

}